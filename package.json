  },
  "homepage": "https://github.com/geekodour/gitpushblog#readme",
  "dependencies": {
    "bulma": "0.4.1",
    "cp": "0.2.0",
    "github-blog-api": "1.1.0",
    "slug": "0.9.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.2",
