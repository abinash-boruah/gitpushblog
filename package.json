{
  "name": "gitpushblog",
  "version": "1.0.0",
  "description": "a git based blog engine - (currently using react)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pregenerate": "npm run clean:dist",
    "generate": "node scripts/generate.js",
